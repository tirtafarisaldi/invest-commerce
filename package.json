{
  "name": "seeds",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:run": "ENVFILE=.env.development react-native run-android",
    "android:run:staging": "ENVFILE=.env.staging react-native run-android",
    "android:run:prod": "ENVFILE=.env.production react-native run-android",
    "android:run:release": "ENVFILE=.env.production react-native run-android --variant=release",
    "android:build:release": "cd android/ && ENVFILE=.env.production ./gradlew app:assembleRelease",
    "android:build:dev": "cd android/ && ENVFILE=.env.development ./gradlew app:assembleRelease",
    "android:build:staging": "cd android/ && ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:build:debug": "ENVFILE=.env.development react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/; cd android && ./gradlew assembleDebug && cd ..",
    "android:bundle:release": "cd android/ && ENVFILE=.env.production ./gradlew app:bundleRelease && cd ..",
    "clean": "watchman watch-del-all && npm cache clean --force && cd android && ./gradlew clean && cd ..",
    "ios": "ENVFILE=.env.development && react-native run-ios",
    "ios:14": "yarn ios --simulator 'iPhone 14'",
    "ios:prod": "ENVFILE=.env.production && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.5",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-native-seoul/masonry-list": "^1.4.1",
    "@react-native/normalize-color": "^2.1.0",
    "@react-navigation/bottom-tabs": "^6.0.3",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.4",
    "@types/react-native-extra-dimensions-android": "^1.2.0",
    "@types/urijs": "^1.19.19",
    "add": "^2.0.6",
    "axios": "^0.26.1",
    "i18next": "^21.6.16",
    "moment": "^2.29.4",
    "react": "17.0.2",
    "react-i18next": "^11.16.7",
    "react-native": "0.66.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-collapsible": "^1.6.0",
    "react-native-config": "^1.5.0",
    "react-native-date-picker": "^4.2.9",
    "react-native-device-info": "^10.3.0",
    "react-native-extra-dimensions-android": "^1.2.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-fbsdk-next": "^11.1.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "^6.1.4",
    "react-native-reanimated": "^3.0.2",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-video": "^5.2.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "urijs": "^1.19.11",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-native-svg-charts": "^5.0.12",
    "@types/react-native-video": "^5.0.14",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
